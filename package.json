{
  "private": true,
  "name": "emilyaviva.com",
  "description": "My professional website - Emily Aviva Kapor-Mater",
  "version": "0.5.1",
  "author": {
    "name": "Emily Aviva Kapor-Mater",
    "email": "emily@emilyaviva.com",
    "url": "http://emilyaviva.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/emilyaviva/emilyaviva.com"
  },
  "bugs": {
    "url": "https://github.com/emilyaviva/emilyaviva.com/issues"
  },
  "keywords": [
    "gatsby",
    "static",
    "professional",
    "personal",
    "website"
  ],
  "license": "ISC",
  "scripts": {
    "build": "gatsby build",
    "clean": "rm -rf public/*",
    "deploy": "gatsby build --prefix-links && gh-pages -d public",
    "dev": "gatsby develop",
    "lint": "standard --verbose | snazzy",
    "serve-build": "NODE_ENV=production gatsby build && gatsby serve-build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "_aerobatic": {
    "router": [
      {
        "module": "webpage"
      },
      {
        "module": "redirect",
        "options": {
          "/about": "/",
          "/portfolio": "/#portfolio",
          "/talks": "/#talks",
          "/contact": "/#contact"
        }
      }
    ],
    "build": {
      "engine": "npm",
      "script": "build",
      "output": "public"
    }
  },
  "dependencies": {
    "anchorate": "^1.1.0",
    "date-fns": "^1.3.0",
    "font-awesome": "^4.6.3",
    "gatsby": "^0.12.9",
    "highlight.js": "^9.7.0",
    "history": "^2.0.0",
    "image-webpack-loader": "^2.0.0",
    "js-yaml": "^3.6.0",
    "markdown-it": "^8.0.0",
    "markdown-it-abbr": "^1.0.4",
    "markdown-it-attrs": "^0.7.1",
    "markdown-it-deflist": "^2.0.1",
    "markdown-it-footnote": "^3.0.1",
    "markdown-it-sub": "^1.0.0",
    "normalize.css": "^4.2.0",
    "react-disqus-thread": "^0.4.0",
    "react-headroom": "^2.1.2",
    "safe-access": "^0.1.0",
    "toml-js": "0.0.8"
  },
  "devDependencies": {
    "babel-eslint": "^6.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-1": "^6.13.0",
    "fs-extra": "^0.30.0",
    "gh-pages": "^0.11.0",
    "snazzy": "^5.0.0",
    "standard": "^8.1.0"
  }
}
